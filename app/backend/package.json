    "@types/sequelize": "4.28.10",
    "nodemon": "2.0.15",
    "sequelize-cli": "6.3.0",
    "ts-node": "10.7.0",
    "ts-node-dev": "1.1.8",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
